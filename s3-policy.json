"Version": "2012-10-17",
"Statement": [
    {
        "Sid": "DelegateS3Access",
        "Effect": "Allow",
        "Principal": {
            "AWS": "ARN-of-User"
        },
        "Action": [
            "s3:ListBucket",
            "s3:GetObject"
        ],
        "Resource": [
            "SOURCE-BUCKET-ARN/*",
            "SOURCE-BUCKET-ARN"
        ]
    }
]
}
